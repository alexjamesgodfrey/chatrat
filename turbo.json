{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DEFAULT_AGENTDB_TOKEN",
    "DEFAULT_AGENTDB_DB_API_KEY",
    "AGENTDB_BASE_URL",
    "SESSION_SECRET",
    "BASE_URL",
    "UPSTASH_REDIS_REST_URL",
    "UPSTASH_REDIS_REST_TOKEN"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**", "dist/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**", "junit.xml?"]
    },
    "format": {
      "outputs": []
    }
  }
}
